fn greet(name) {
   return "Hello, {name}"
}

let greeting = greet("James")
println(greeting)

fn select(lambda, left, right) {
   return lambda(left, right)
}

let lefty = select(|left, right| left, "Left", "Right")
let righty = select(|left, right| right, "Left", "Right")
println(lefty)
println(righty)